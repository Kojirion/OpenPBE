FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      n0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.0021042e-08;

boundaryField
{
    outlet
    {
        type               inletOutlet;
        inletValue          uniform 0.0021042;

    }
    walls
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 0.0021042;
    }

    frontAndBack
    {
        type            empty;
    }
}


